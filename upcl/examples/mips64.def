arch "mips64" {

	name "MIPS R4000 (64bit)";

	endian both;
  default_endian big;

	byte_size 8;
	word_size 64;
	address_size 64;
	float_size 64;

	register_file {

    // General Purpose Register Group
		group R {
      // r0 is hardwired to zero.
      [ #i64 r0 <- 0 ],
      // define r1..r31 as 32bit registers.
      [ 31 ** #i64 r?:1 ]
    };

    // Special Register Group
		group S {

			[ #i64 pc -> %PC ],
			[ #i64 npc -> %NPC ]

		};

	};

};

//include "mips.def";

insn a : ;

